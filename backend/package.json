{
  "name": "tarefas-backend",
  "version": "0.0.1",
  "description": "Tarefas backend",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "prettier": "prettier --single-quote --trailing-comma all --write \"src/app/**/*.ts\""
  },
  "private": true,
  "nodemonConfig": {
    "watch": [
      "./src/app"
    ],
    "ext": "ts",
    "ignore": [
      "./src/app/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/app/index.ts",
    "verbose": true
  },
  "dependencies": {
    "@types/passport-local": "^1.0.33",
    "body-parser": "1.19.0",
    "cluster": "0.7.7",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cron": "1.7.1",
    "express": "4.17.0",
    "fluentreports": "1.2.5",
    "generate-password": "1.4.1",
    "helmet": "3.18.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "mysql": "2.17.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rotating-file-stream": "1.4.1",
    "rxjs": "6.5.2",
    "sqlite3": "4.0.8",
    "swagger-ui-express": "^4.0.4",
    "typeorm": "0.2.17",
    "underscore": "1.9.1",
    "winston": "3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/cookie-parser": "1.4.1",
    "@types/cron": "1.7.1",
    "@types/helmet": "0.0.43",
    "@types/jsonwebtoken": "8.3.2",
    "@types/mocha": "5.2.6",
    "@types/morgan": "1.7.35",
    "@types/node": "12.0.2",
    "@types/passport-jwt": "3.0.1",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/yamljs": "^0.2.30",
    "braces": "^2.3.1",
    "chai": "4.2.0",
    "forever": "1.0.0",
    "mocha": "6.1.4",
    "node": "10.15.3",
    "nodemon": "1.19.0",
    "npm": "6.13.4",
    "prettier": "1.17.1",
    "ts-node": "8.1.0",
    "typescript": "3.4.5"
  }
}
